<svg xmlns="http://www.w3.org/2000/svg" width="192" height="192" viewBox="0 0 192 192">
  <defs>
    <filter id="shadow">
      <feGaussianBlur in="SourceAlpha" stdDeviation="3"/>
      <feOffset dx="2" dy="2" result="offsetblur"/>
      <feComponentTransfer>
        <feFuncA type="linear" slope="0.3"/>
      </feComponentTransfer>
      <feMerge> 
        <feMergeNode/>
        <feMergeNode in="SourceGraphic"/> 
      </feMerge>
    </filter>
  </defs>
  
  <!-- Background circle -->
  <circle cx="96" cy="96" r="88" fill="#E0E5EC"/>
  
  <!-- Breathing waves (3 concentric circles) -->
  <circle cx="96" cy="96" r="45" fill="none" stroke="#8daaf5" stroke-width="3" opacity="0.8"/>
  <circle cx="96" cy="96" r="30" fill="none" stroke="#8daaf5" stroke-width="2.5" opacity="0.6"/>
  <circle cx="96" cy="96" r="15" fill="none" stroke="#8daaf5" stroke-width="2" opacity="0.4"/>
  
  <!-- Center dot -->
  <circle cx="96" cy="96" r="8" fill="#8daaf5" filter="url(#shadow)"/>
</svg>
